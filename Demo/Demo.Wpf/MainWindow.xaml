<Window x:Class="Demo.Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Demo.Wpf"
        mc:Ignorable="d"
        Title="MainWindow"
        Height="350"
        Width="525">
    <Grid>
        <local:OutlinedTextBlock Text="Doge"
                                 Stroke="Red"
                                 Fill="Black"
                                 StrokeThickness="1"
                                 FontSize="22"></local:OutlinedTextBlock>
        <local:OutlineTextBlock2 Text="10"
                                 HorizontalAlignment="Center"
                                 VerticalAlignment="Center"
                                 FontSize="40"
                                 StrokeThickness="2"
                                 x:Name="OutlineTextBlock2"
                                 Foreground="Red"
                                 Stroke="Black"></local:OutlineTextBlock2>
        <Button VerticalAlignment="Bottom"
                Content="Doge"
                Click="ButtonBase_OnClick"></Button>
    </Grid>
</Window>